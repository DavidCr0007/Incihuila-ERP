{
  "name": "ERP Multisede",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/workspace/erp-multisede",
  "shutdownAction": "stopCompose",
  "postCreateCommand": "pip install -r requirements/dev.txt",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "batisteo.vscode-django",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "remoteEnv": {
    "DJANGO_SETTINGS_MODULE": "erp.settings.local"
  }
}
